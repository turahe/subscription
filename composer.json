{
    "name": "turahe/subscription",
    "description": "A flexible and feature-rich subscription management system for Laravel applications",
    "version": "1.2.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Nur Wachid",
            "email": "wachid@outlook.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "spatie/eloquent-sortable": "^4.4",
        "spatie/laravel-sluggable": "^3.6",
        "turahe/laravel-userstamps": "^1.0",
        "turahe/master-data": "^1.0"
    },
    "require-dev": {
        "laravel/pint": "^1.17",
        "orchestra/testbench": "^10.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Turahe\\Subscription\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Turahe\\Subscription\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "aliases": {},
            "providers": [
                "Turahe\\Subscription\\SubscriptionServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test:coverage": "vendor/bin/phpunit --coverage-html coverage"
    }
}
